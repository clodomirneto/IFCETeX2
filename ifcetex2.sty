\newcommand{\trabalhoacademico}[1]{
	\setboolean{ifcetex2@ehtccgraduacao}{false}
	\setboolean{ifcetex2@ehtccespecializacao}{false}
	\setboolean{ifcetex2@ehdissertacao}{false}
	\setboolean{ifcetex2@ehtese}{false}
	
	\ifthenelse{\equal{#1}{tccgraduacao}}{%
		\setboolean{ifcetex2@ehtccgraduacao}{true}
	}{
		\ifthenelse{\equal{#1}{tccespecializacao}}{%
			\setboolean{ifcetex2@ehtccespecializacao}{true}
		}{
			\ifthenelse{\equal{#1}{dissertacao}}{%
				\setboolean{ifcetex2@ehdissertacao}{true}
			}{
				\ifthenelse{\equal{#1}{tese}}{%
					\setboolean{ifcetex2@ehtese}{true}
				}{
					\setboolean{ifcetex2@ehdissertacao}{true}
				}
			}
		}	
	}
}

